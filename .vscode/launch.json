  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "ZGQ Scrapor",
      "program": "${workspaceFolder}\\src\\scrape.js",
      "args": [
        "--wkdir=${workspaceFolder}\\wkdir"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "ZGQ Inbounder",
      "program": "${workspaceFolder}\\src\\inbound.js",
      "args": [
        "--wkdir=${workspaceFolder}\\wkdir",
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "ZGQ Updater",
      "program": "${workspaceFolder}\\src\\update.js",
      "args": [
        "--wkdir=${workspaceFolder}\\wkdir",
      ]
    }
  ]
}